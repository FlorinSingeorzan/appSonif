<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<QuickView id="quickView">
		<QuickViewPage
			pageId="{pageId}"
			icon="{icon}"
			groups="{
				path: 'groups',
				templateShareable: false
			}">
			<QuickViewGroup
				heading="{heading}"
				elements="{
					path: 'elements',
					templateShareable: false
				}">
				<QuickViewGroupElement
					label="{label}"
					value="{value}">
				</QuickViewGroupElement>
			</QuickViewGroup>
		</QuickViewPage>
	</QuickView>
</core:FragmentDefinition>