<mvc:View controllerName="sap.Sonif.controller.VCurrency"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml">

	<Page showHeader="false" backgroundDesign="Info">
		<!-- <subHeader class="subHeaderDesign"> -->
		<mvc:XMLView viewName="sap.Sonif.view.Toolbar" />
		<!-- </subHeader> -->
		<content>
			<HBox>
				<items>
					<VBox width="auto" wrap="Wrap">
						<items>
							<HBox width="auto" wrap="Wrap">

								<items>

									<GenericTile class="sapUiSmallMarginBegin sapUiMediumMarginTop"
										backgroundImage="images/btc.png" press="btc"></GenericTile>
									<GenericTile class="sapUiSmallMarginBegin  sapUiMediumMarginTop"
										backgroundImage="images/eth.png" press="eth"></GenericTile>
									<GenericTile class="sapUiSmallMarginBegin  sapUiMediumMarginTop"
										backgroundImage="images/xrp.png" press="xrp"></GenericTile>
									<GenericTile class="sapUiSmallMarginBegin  sapUiMediumMarginTop"
										backgroundImage="images/eos.png" press="eos"></GenericTile>

								</items>

							</HBox>
							<HBox width="auto" direction="Column" id="__vbox0">
								<items>
									<HBox wrap="Wrap">
										<items>
											<GenericTile class="sapUiSmallMarginBegin sapUiMediumMarginTop"
												backgroundImage="images/iota.png" press="iota"></GenericTile>
											<GenericTile class="sapUiSmallMarginBegin sapUiMediumMarginTop"
												backgroundImage="images/neo.png" press="neo"></GenericTile>
											<GenericTile class="sapUiSmallMarginBegin sapUiMediumMarginTop"
												backgroundImage="images/xmr.png" press="xmr"></GenericTile>
											<GenericTile class="sapUiSmallMarginBegin sapUiMediumMarginTop"
												backgroundImage="images/dash.png" press="dash"></GenericTile>
										</items>
									</HBox>
									<HBox wrap="Wrap">
										<items>
											<GenericTile
												class="sapUiSmallMarginBegin sapUiMediumMarginTop"
												backgroundImage="images/trx.jpg" press="trx"></GenericTile>
											<GenericTile class="sapUiSmallMarginBegin  sapUiMediumMarginTop"
												backgroundImage="images/ltc.png" press="ltc"></GenericTile>
											<GenericTile class="sapUiSmallMarginBegin  sapUiMediumMarginTop"
												backgroundImage="images/zec.png" press="zec"></GenericTile>
											<GenericTile class="sapUiSmallMarginBegin  sapUiMediumMarginTop"
												backgroundImage="images/bch.png" press="bth"></GenericTile>
										</items>
									</HBox>
								</items>
							</HBox>

						</items>
					</VBox>
					<FlexBox id="coin" class="sapUiMediumMarginTop sapUiMediumMarginBegin"
						 direction="Column" justifyContent="Start" width="700px" height="auto">
						<html:script  src="controller/bitcoin.js" type="application/javascript"></html:script>
					</FlexBox>
				</items>
			</HBox>
		</content>
	</Page>

</mvc:View>
