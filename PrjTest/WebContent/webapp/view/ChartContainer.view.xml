<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:suite="sap.suite.ui.commons"
	controllerName="sap.suite.ui.commons.PrjTest.controller.ChartContainer">
	<Page
		title="{i18n>chartContainerPageTitle}"
		showNavButton="true"
		navButtonPress="onNavButtonPressed"
		backgroundDesign="Solid">
		<content>
			<l:Grid defaultSpan="L12 M12 S12"
				class="sapUiResponsiveMargin"
				vSpacing="0"
				hSpacing="0">
				<suite:ChartContainer
					title="{
						parts: [
							'i18n>chartContainerBulletChartTitle',
							'business>/valueUnit'
						],
						formatter: 'jQuery.sap.formatMessage'
					}">
					<suite:ChartContainerContent
						icon="sap-icon://vertical-bullet-chart"
						title="{i18n>chartContainerBulletChart3months}">
						<suite:content>
						<core:Fragment
								fragmentName="sap.suite.ui.commons.PrjTest.fragment.VizChart"
								type="JS" />
						</suite:content>
					</suite:ChartContainerContent>
					<suite:ChartContainerContent
						icon="sap-icon://vertical-bullet-chart"
						title="{i18n>chartContainerBulletChart6months}">
						<suite:content>
						<core:Fragment
								fragmentName="sap.suite.ui.commons.PrjTest.fragment.VizChart"
								type="JS" />
						</suite:content>
					</suite:ChartContainerContent>
					<suite:ChartContainerContent
						icon="sap-icon://vertical-bullet-chart"
						title="{i18n>chartContainerBulletChart12months}">
						<suite:content>
							<core:Fragment
								fragmentName="sap.suite.ui.commons.PrjTest.fragment.VizChart"
								type="JS" />
						</suite:content>
					</suite:ChartContainerContent>
					<suite:ChartContainerContent
						icon="sap-icon://table-chart"
						title="{i18n>chartContainerTableContentTitle}">
						<suite:content>
							<Table items="{/Values}">
								<columns>
									<Column>
										<Text text="{i18n>chartContainerPrjTestId}" />
									</Column>
									<Column>
										<Text text="{i18n>chartContainerBitcoinValues}" />
									</Column>
								</columns>
								<items>
									<ColumnListItem>
										<cells>
											<Text text="{month}" />
											<Text text="{value}" />
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</suite:content>
					</suite:ChartContainerContent>
				</suite:ChartContainer>
			</l:Grid>
		</content>
	</Page>
</mvc:View>