<mvc:View height="100%"
		controllerName="sap.Sonif.controller.ProcessFlow"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:f="sap.ui.layout.form">
	<Page class="sapUiContentPadding" showHeader="false">
		<mvc:XMLView viewName="sap.Sonif.view.Toolbar" />
		<content>
			<f:SimpleForm labelSpanL="6"
						labelSpanM="6"
						editable="true"
						layout="ResponsiveGridLayout">
			</f:SimpleForm>
				<Panel id="carouselContainer" height="600px">
					<Carousel class="sapUiContentPadding" loop="true" id="carouselSample" >
					</Carousel>
				</Panel>
		</content>
	</Page>
</mvc:View>