<mvc:View height="100%"
		controllerName="sap.suite.ui.commons.PrjTest.controller.ProcessFlow"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:f="sap.ui.layout.form">
	<Page class="sapUiContentPadding" showHeader="false">
		<mvc:XMLView viewName="sap.suite.ui.commons.PrjTest.view.Toolbar" />
		<content>
			<f:SimpleForm labelSpanL="6"
						labelSpanM="6"
						editable="true"
						layout="ResponsiveGridLayout">
				<Label text="Shrink carousel container" />
				<Slider value="5"
						liveChange="onSliderMoved"
						width="320px"
						step="1"
						min="1"
						max="5"/>
			</f:SimpleForm>
				<Panel id="carouselContainer" height="650px">
					<Carousel class="sapUiContentPadding" loop="true" id="carouselSample" >
					</Carousel>
				</Panel>
		</content>
	</Page>
</mvc:View>
